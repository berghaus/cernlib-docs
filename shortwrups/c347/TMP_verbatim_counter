<tex2html_mydb_mark>#34#$\mathbf{G}(k',p,a,b)$
<tex2html_mydb_mark>#35#\begin{displaymath}
\begin{array}{rcl}
\lambda \mathrm{K}(k) + \mu \mathrm{E}(k) & = &
\mathbf{G}(k',1,\lambda+\mu,\lambda+\mu {k'}^2) \\\lambda \mathrm{K}(k) + \mu \Pi(h,k) & = &
\mathbf{G}(k',h+1,\lambda+\mu,\lambda (h+1)+\mu)
\end{array}
\end{displaymath}
<tex2html_mydb_mark>#36#\begin{displaymath}
\begin{array}{rcl}
\mathrm{K}(k)                           & = & \mathbf{G}(k',1,1,1), \\\mathrm{E}(k)                           & = & \mathbf{G}(k',1,1,{k'}^2)\\(\mathrm{K}(k)-\mathrm{E}(k))/k^2       & = & \mathbf{G}(k',1,0,1), \\(\mathrm{K}(k)-{k'}^2\mathrm{E}(k))/k^2 & = & \mathbf{G}(k',1,1,0), \\\Pi(h,k)                                & = & \mathbf{G}(k',h+1,1,1),\\(\mathrm{K}(k)-\Pi(h,k))/h              & = & \mathbf{G}(k',h+1,0,1),\\\end{array}
\end{displaymath}
