% First definition to set up unequivocal commands
\newcommand{\DEFCMD}[4]{%
       {\def_{}\xdef\tmp{#1}}% label
       \def\_{\char'137\relax}
        \expandafter\gdef\csname\tmp\endcsname{{\tt#3}\CMIX{X}{#3}{#2}}}

